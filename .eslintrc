{
    "parser": "babel-eslint",
    "extends": "react-app",
    "env": {
        "browser": true,
        "mocha": true,
        "node": true
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "quotes": [
            2,
            "single"
        ],
        "eol-last": [
            0
        ],
        "consistent-return": [
            0
        ],
        "import/no-unresolved": [
            0
        ],
        "no-mixed-requires": [
            0
        ],
        "no-underscore-dangle": [
            0
        ],
        "no-unused-vars": [
            0
        ],
        "semi": [
            0
        ]
    }
}